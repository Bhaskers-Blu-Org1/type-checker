{
  "name": "type-checker",
  "version": "0.2.1",
  "description": "Check if all of your typography complies with IBM Type. Just press CTRL+T.",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "commitizen": "2.9.6",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "firefox-extension-deploy": "1.1.0",
    "node-sass": "^4.6.1",
    "sass-loader": "^6.0.6",
    "scss-loader": "0.0.1",
    "style-loader": "^0.19.0",
    "web-ext": "2.0.0",
    "webpack": "^3.8.1",
    "webpack-cleanup-plugin": "^0.5.1"
  },
  "scripts": {
    "test": "echo 'Tests coming soon'",
    "dev": "concurrently --kill-others \"npm run build:watch\" \"npm run ext:watch\"",
    "build": "webpack",
    "build:watch": "webpack --watch",
    "ext:watch": "web-ext run -s ./dist -u https://ibm.github.io/type/ --bc",
    "cz": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IBM/type-checker.git"
  },
  "keywords": [
    "IBM",
    "type",
    "typography",
    "web",
    "extension"
